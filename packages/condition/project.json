{
  "name": "condition",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/condition/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/condition",
        "main": "packages/condition/src/index.ts",
        "tsConfig": "packages/condition/tsconfig.lib.json",
        "assets": [
          "packages/condition/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/condition/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/condition/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
