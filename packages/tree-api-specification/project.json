{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/tree-api-specification/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/tree-api-specification",
        "main": "packages/tree-api-specification/src/index.ts",
        "tsConfig": "packages/tree-api-specification/tsconfig.lib.json",
        "assets": ["packages/tree-api-specification/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/tree-api-specification/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nxext/vitest:vitest",
      "options": {
        "command": "run"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ]
    },
    "typecheck": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "tsc -p tsconfig.lib.json",
        "cwd": "packages/tree-api-specification"
      },
      "dependsOn": [{ "projects": "dependencies", "target": "build" }]
    }
  },
  "tags": []
}
