{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/builder",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["pnpm nx codegen builder", "pnpm nx build:next builder"]
      }
    },
    "build:next": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/builder",
        "outputPath": "dist/apps/builder"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "builder:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "builder:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "builder:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "builder:build:production"
      }
    },
    "test": {
      "executor": "@nxext/vitest:vitest",
      "options": {
        "command": "run"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/builder/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
